# LangSmith Configuration (for tracing and monitoring)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=test_agent_supervisor
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# LangChain Configuration (automatically set by tracing setup)
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_PROJECT=test_agent_supervisor
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_SESSION_TOKEN=your_aws_session_token_here

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# Google Search API (for web search functionality)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_google_custom_search_engine_id_here

# Tavily API Key (alternative search functionality)
TAVILY_API_KEY=your_tavily_api_key_here

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=password
